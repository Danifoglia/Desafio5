
<h1>Lista de productos:</h1>
{{#if length}}
    <div class="table-responsive">
        <table class="table table-dark">
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto</th>
            </tr>
            {{#each productos}}
                 
                <tr>
                    <th>Nombre: {{this.title}}</th>
                    <th>Precio: ${{this.price}}</th>
                    <th>Imagen: <img src="{{this.thumbnail}}" alt="" height="50px", width="50px" /></th>
                </tr>
                
            {{/each}} 
        </table>
    {{else}}
        <br>
       <h2>No hay productos en la lista!</h2> 
{{/if}}




